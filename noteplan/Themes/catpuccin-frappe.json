{
  "name": "Catpuccin Frappe",
  "style": "Dark",
  "author": {
    "name": "Ashley Trinh",
    "email": "itsashley@hey.com"
  },
  "editor": {
    "backgroundColor": "#242633",
    "altBackgroundColor": "#313445",
    "tintColor": "#91ACEE",
    "tintColor2": "#E3A37D",
    "textColor": "#EDD6D0",
    "toolbarBackgroundColor": "#424557",
    "toolbarIconColor": "#7685F5",
    "menuItemColor": "#EDD6D0",
    "timeBlockColor": "#E3A37D",
    "shouldOverwriteFont": false
  },
  "styles": {
    "body": {
      "font": "MonaspaceArgonVar-Regular",
      "size": 13,
      "color": "#EDD6D0",
      "lineSpacing": -3
    },
    "tabbed": {
      "lineSpacing": -3,
      "headIndent": 20
    },
    "title1": {
      "color": "#C3A0E1",
      "size": 20,
      "font": "MonaspaceArgonVar-WideBold",
      "lineSpacing": -3
    },
    "title-mark1": {
      "color": "#4F4A6A",
      "size": 20,
      "font": "MonaspaceArgonVar-WideBold",
      "lineSpacing": -3
    },
    "title2": {
      "color": "#93E2D5",
      "size": 16,
      "font": "MonaspaceXenonVar-Regular",
      "lineSpacing": -3
    },
    "title-mark2": {
      "color": "#4F4A6A",
      "size": 16,
      "font": "MonaspaceArgonVar-WideMedium",
      "paragraphSpacingBefore": 28,
      "lineSpacing": -3
    },
    "title3": {
      "color": "#CDD6F4",
      "size": 14,
      "font": "MonaspaceRadonVar-Italic",
      "lineSpacing": -3
    },
    "title-mark3": {
      "color": "#4F4A6A",
      "size": 14,
      "font": "MonaspaceRadonVar-Italic",
      "paragraphSpacingBefore": 8,
      "lineSpacing": -3
    },
    "title4": {
      "color": "#85C1DC",
      "size": 13,
      "font": "MonaspaceArgonVar-LightItalic",
      "lineSpacing": -3
    },
    "title-mark4": {
      "color": "#4F4A6A",
      "size": 13,
      "font": "MonaspaceArgonVar-LightItalic",
      "paragraphSpacingBefore": 10,
      "lineSpacing": -3
    },
    "bold": {
      "color": "#FFAF7F",
      "font": "MonaspaceArgonVar-Bold"
    },
    "bold-left-mark": {
      "color": "#4F4A6A"
    },
    "bold-right-mark": {
      "color": "#4F4A6A"
    },
    "italic": {
      "color": "#94E59A",
      "font": "MonaspaceArgonVar-Italic"
    },
    "italic-left-mark": {
      "color": "#4F4A6A"
    },
    "italic-right-mark": {
      "color": "#4F4A6A"
    },
    "boldItalic": {
      "color": "#E590A8",
      "font": "MonaspaceArgonVar-BoldItalic"
    },
    "boldItalic-left-mark": {
      "color": "#4F4A6A"
    },
    "boldItalic-right-mark": {
      "color": "#4F4A6A"
    },
    "code": {
      "font": "MonaspaceKrypton-Regular",
      "color": "#9CD3CA",
      "size": 13
    },
    "code-left-backtick": {
      "color": "#4F4A6A"
    },
    "code-right-backtick": {
      "color": "#4F4A6A"
    },
    "code-fence": {
      "body": {
        "font": "MonaspaceKrypton-Regular",
        "size": 13,
        "lineSpacing": -5
      },
      "language-specifier": {
        "font": "MonaspaceRadonVar-Light",
        "color": "#A4A1B7",
        "size": 12,
        "lineSpacing": -8
      },
      "fence-open": {
        "color": "#4F4A6A"
      },
      "fence-close": {
        "color": "#4F4A6A"
      }
    },
    "checked": {
      "color": "#AFD090",
      "headIndent": 28,
      "lineSpacing": -3
    },
    "todo": {
      "font": "noteplanstate",
      "color": "#D08E7C",
      "headIndent": 28,
      "lineSpacing": -3
    },
    "checked-todo-characters": {
      "font": "noteplanstate"
    },
    "special-char": {
      "font": "MonaspaceArgonVar-Regular"
    },
    "quote-mark": {
      "color": "#5D5F75",
      "font": "noteplanstate",
      "headIndent": 28,
      "lineSpacing": -8
    },
    "quote-content": {
      "color": "#6D6F83",
      "type": "italic",
      "lineSpacing": -4,
      "headIndent": 2,
      "size": 12
    },
    "flagged-1": {
      "backgroundColor": "#313244"
    },
    "flagged-2": {
      "backgroundColor": "#713255"
    },
    "flagged-3": {
      "backgroundColor": "#BA2C46"
    },
    "working-on": {
      "backgroundColor": "#77ABF2",
      "color": "#232634"
    },
    "strikethrough": {
      "color": "#62607E",
      "strikethroughColor": "#62607E"
    },
    "strikethrough-left-tilde": {
      "color": "#4F4A6A"
    },
    "strikethrough-right-tilde": {
      "color": "#4F4A6A"
    },
    "underline": {
      "underlineStyle": 2,
      "underlineColor": "#FF84A8"
    },
    "underline-left-tilde": {
      "color": "#44333333"
    },
    "underline-right-tilde": {
      "color": "#44333333"
    },
    "highlighted": {
      "regex": "(::)(.+?)(::)",
      "matchposition": 2,
      "isRevealOnCursorRange": true,
      "backgroundColor": "#F9E2AF"
    },
    "highlighted-left-colon": {
      "regex": "(::)(.+?)(::)",
      "matchposition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#44333333",
      "backgroundColor": "#F9E2AF"
    },
    "highlighted-right-colon": {
      "regex": "(::)(.+?)(::)",
      "matchposition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#44333333",
      "backgroundColor": "#F9E2AF"
    },
    "link": {},
    "schedule-to-date-link": {},
    "done-date": {},
    "schedule-from-date-link": {},
    "note-title-link": {},
    "hashtag": {},
    "attag": {},
    "phonenumber": {}
  }
}
