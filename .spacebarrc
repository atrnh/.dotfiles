#!/usr/bin/env sh

spacebar -m config position                    bottom
spacebar -m config display                     all
spacebar -m config height                      26
spacebar -m config title                       on
spacebar -m config spaces                      on
spacebar -m config clock                       on
spacebar -m config power                       on
spacebar -m config padding_left                20
spacebar -m config padding_right               20
spacebar -m config spacing_left                25
spacebar -m config spacing_right               15
spacebar -m config text_font                   "FantasqueSansMono Nerd Font:Regular:14.0"
spacebar -m config icon_font                   "FantasqueSansMono Nerd Font:Regular:14.0"
spacebar -m config background_color            0xff202020
spacebar -m config foreground_color            0xffffffff
spacebar -m config power_icon_color            0xffFF7EE3
spacebar -m config battery_icon_color          0xffFF7EE3
spacebar -m config dnd_icon_color              0xff4FC1FF
spacebar -m config clock_icon_color            0xff3DDAB8
spacebar -m config power_icon_strip             
spacebar -m config space_icon                  •
spacebar -m config space_icon_color            0xff4FC1FF
spacebar -m config space_icon_color_secondary  0xff4FC1FF
spacebar -m config space_icon_color_tertiary   0xfffff9b0
spacebar -m config space_icon_strip             爵    6 7 8 9 10
spacebar -m config spaces_for_all_displays     on
spacebar -m config clock_icon                  
spacebar -m config dnd_icon                    
spacebar -m config clock_format                "%a %b %d • %I:%M %p"

spacebar -m config right_shell                 off
spacebar -m config right_shell_icon            
spacebar -m config right_shell_icon_color      0xffB7BEFF
spacebar -m config right_shell_command         "python3 ~/.dotfiles/scripts/time_to_eastern.py"

spacebar -m config left_shell                 off
spacebar -m config left_shell_icon            
spacebar -m config left_shell_icon_color      0xffB7BEFF
spacebar -m config left_shell_command         "gcalcli --nocolor --config-folder=~/.config/gcalcli --default-calendar=\"Ashley Trinh\" agenda --nostarted \"`date`\" | head -2 | tail -1 | tr -s ' ' | cut -d' ' -f4- | cut -d' ' -f1-2"
echo "spacebar configuration loaded.."
